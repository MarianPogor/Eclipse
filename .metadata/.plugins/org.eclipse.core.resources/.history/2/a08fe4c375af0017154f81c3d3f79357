package Ex10_4;

public class fibonnaci
{

  
   
      public int fibonacci(int n)  {
         if(n == 0)
             return 0;
         else if(n == 1)
           return 1;
        else
           return fibonacci(n - 1) + fibonacci(n - 2);
     }
      
      public static void main(String[] args)
      {
         fibonnaci f1= new fibonnaci();
         
         f1.fibonacci(6);
         System.out.println(f1);
      }

   }


